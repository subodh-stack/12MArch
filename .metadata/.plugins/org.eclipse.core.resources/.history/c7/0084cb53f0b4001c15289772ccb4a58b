package BasicJ;

public class Cons {
	
	Cons()//will be called only when we create object
	{
		System.out.println("Hello i am constructor");
	}
	
	int a =10;
	
	String b = "Velocity";
	
	public void test() {
		System.out.println("Hello");
	}
	
	
	public static void main(String[] args) {
		Cons obj = new Cons();//default constructor
		System.out.println(obj.a);
		System.out.println(obj.b);
		obj.test();
		obj.test();
		obj.test();
		
		Cons obj2 = new Cons();
		
	}
	
}
