package Day1;

public class LocalVariable {
	static int b = 30;
	
	public void demo()
	{
		int a =10; //local variable cannot be accessed outside the method
		System.out.println(a);
	}
	
	public void test() {
		System.out.println(b);
		System.out.println(a);
	}
	
	
	public static void main(String[] args) {
		LocalVariable obj = new LocalVariable();
		obj.demo();
	}
	
}
