package BasicJ;

public class Cons {
	
	int a =10;
	
	String b = "Velocity";
	
	public void test() {
		System.out.println("Hello");
	}
	
	
	public static void main(String[] args) {
		Cons obj = new Cons();//default constructor
		System.out.println(obj.a);
		System.out.println(obj.b);
		obj.test();
	}
	
}
